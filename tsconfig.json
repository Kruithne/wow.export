{
	"compilerOptions": {
		"lib": [
			"ESNext",
			"DOM",
		],
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "bundler",
		"strict": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"emitDeclarationOnly": true,
		"declaration": true,
		"allowJs": true
	}
}